package com.z;

public class TestCat {

	public static void main(String[] args) {
		//类名 对象名 = new 类名（）；
		Cat tom = new Cat ();
		//对象名.属性名 访问属性
		tom.name = "汤姆";
		tom.age = 2;
		tom.color = "蓝色";
		
		System.out.println(tom.name);
		System.out.println(tom.age);
		System.out.println(tom.color);
		
		//对象名.方法名 调用方法
		tom.sayHello();
		int r = tom.add(1, 2);
		System.out.println(r);
		
	}
}
